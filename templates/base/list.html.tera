{% extends "base/base" %}

{% block content %}
<input class="form-control mb-4" type="text" placeholder="Search.." onkeyup="filter_list(this, 'searchable-list');">

<ul class="list-group" id="searchable-list">
    {% block items %}{% endblock %}
</ul>
{% endblock %}

{% block javascript %}
<script src="/static/custom/filter-list.js"></script>
{% endblock %}
